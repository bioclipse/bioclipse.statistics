<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="net.bioclipse.statistics_feature"
      label="Bioclipse Statistics"
      version="2.6.1.qualifier"
      provider-name="The Bioclipse Project"
      plugin="net.bioclipse.statistics">
   <install-handler library="net.bioclipse.statistics/"/>

   <description>
      Feature that extends Bioclipse with statistics related functionality.
   </description>

   <copyright>
      Copyright (c) 2009 The Bioclipse Project Team and others.
All rights reserved.
   </copyright>

   <license url="http://www.eclipse.org/legal/epl-v10.html">
      The Eclipse Public License + exception to allow GPL plugins to use this feature as explained in the first Bioclipse paper.
   </license>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="net.bioclipse.core"/>
      <import plugin="org.springframework.bundle.spring.aop"/>
      <import plugin="net.sf.cglib"/>
      <import plugin="org.springframework.osgi.aopalliance.osgi"/>
      <import plugin="log4j.over.slf4j"/>
      <import plugin="net.bioclipse.jama" version="1.0.2" match="greaterOrEqual"/>
      <import plugin="net.bioclipse.scripting"/>
      <import plugin="org.jfreechart"/>
   </requires>

   <plugin
         id="net.bioclipse.statistics"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="net.bioclipse.statistics.jama"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="net.bioclipse.chart"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

   <plugin
         id="net.bioclipse.statistics.business"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="net.bioclipse.jama"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="net.bioclipse.chart.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
